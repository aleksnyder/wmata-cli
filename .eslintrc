{
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2020
  },
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "env": {
    "node": true
  },
  "rules": {
    "arrow-parens": "off",
    "consistent-return": "off",
    "camelcase": "off",
    "import/no-dynamic-require": "off",
    "global-require": "off",
    "no-underscore-dangle": "off",
    "no-console": "off"
  },
  "plugins": [
    "prettier"
  ]
}