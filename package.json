{
  "name": "wmata-cli",
  "description": "A CLI for WMATA",
  "license": "MIT",
  "author": "Alek Snyder <aleksnyder@outlook.com> (https://www.aleksnyder.net)",
  "homepage": "https://github.com/aleksnyder/wmata-cli",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aleksnyder/wmata-cli.git"
  },
  "bugs": {
    "url": "https://github.com/aleksnyder/wmata-cli/issues"
  },
  "version": "0.0.1",
  "main": "./dist/index.js",
  "bin": {
    "wmata-cli": "./dist/index.js",
    "wcli": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && webpack --config webpack.config.js -p",
    "clean": "rimraf dist packed",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "pack": "pkg . --out-path packed",
    "test": "npm run lint:fix && npm run testonly:cov",
    "testonly": "NODE_ENV=test jest",
    "testonly:cov": "jest --coverage --runInBand --forceExit --no-cache",
    "testonly:watch": "jest --watch"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-register": "^6.26.0",
    "chalk": "^2.3.0",
    "cli-table3": "^0.5.0",
    "commander": "^2.18.0",
    "global": "^4.3.2",
    "ora": "^1.3.0",
    "path": "^0.12.7",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "copy-webpack-plugin": "^4.3.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^21.2.1",
    "pkg": "^4.3.0-beta.1",
    "prettier": "^1.8.2",
    "prettier-package-json": "^1.4.0",
    "rimraf": "^2.6.2",
    "webpack-node-externals": "^1.6.0"
  },
  "keywords": [
    "WMATA",
    "Metro",
    "DC",
    "cli"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/"
    ],
    "testPathIgnorePatterns": [
      "node_modules/"
    ]
  },
  "pkg": {
    "scripts": [
      "dist/**/*.js"
    ],
    "targets": [
      "node8-macos",
      "node8-linux",
      "node8-win"
    ]
  }
}
