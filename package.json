{
  "name": "wmata-cli",
  "type": "module",
  "description": "A CLI for WMATA",
  "license": "MIT",
  "author": "Alek Snyder <aleksnyder@outlook.com> (https://www.aleksnyder.net)",
  "homepage": "https://github.com/aleksnyder/wmata-cli",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aleksnyder/wmata-cli.git"
  },
  "bugs": {
    "url": "https://github.com/aleksnyder/wmata-cli/issues"
  },
  "version": "0.0.11",
  "main": "./dist/cli.js",
  "bin": {
    "wmata-cli": "./dist/cli.js",
    "wcli": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && webpack --config webpack.config.js -p",
    "clean": "rimraf dist packed",
    "localdev": "NODE_ENV=development node bin/cli.js",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "pack": "pkg . --out-path packed",
    "prepublishOnly": "npm run build && echo '#!/usr/bin/env node' | cat - dist/index.js > temp && mv temp dist/cli.js",
    "test": "npm run lint:fix && npm run testonly:cov",
    "testonly": "NODE_ENV=test jest",
    "testonly:cov": "jest --coverage --runInBand --forceExit --no-cache",
    "testonly:watch": "jest --watch"
  },
  "dependencies": {
    "@babel/register": "^7.0.0",
    "axios": "^0.21.0",
    "chalk": "^2.3.0",
    "cli-table3": "^0.5.1",
    "commander": "^2.19.0",
    "inquirer": "^6.2.1",
    "ora": "^1.3.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^4.6.0",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.4.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.3",
    "jest": "^23.6.0",
    "pkg": "^4.3.7",
    "prettier": "^1.8.2",
    "prettier-package-json": "^1.4.0",
    "rimraf": "^3.0.2",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "resolutions": {
    "braces": "^2.3.1",
    "serialize-javascript": "^3.1.0",
    "yargs-parser": "^13.1.2",
    "mem": "^4.0.0"
  },
  "keywords": [
    "WMATA",
    "Metro",
    "DC",
    "cli"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/"
    ],
    "testPathIgnorePatterns": [
      "node_modules/"
    ]
  },
  "pkg": {
    "scripts": [
      "dist/**/*.js"
    ],
    "targets": [
      "node12-macos",
      "node12-linux",
      "node12-win"
    ]
  }
}
