{
  "name": "wmata-cli",
  "type": "module",
  "description": "A CLI for WMATA",
  "license": "MIT",
  "author": "Alek Snyder <aleksnyder@outlook.com> (https://www.aleksnyder.net)",
  "homepage": "https://github.com/aleksnyder/wmata-cli",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aleksnyder/wmata-cli.git"
  },
  "bugs": {
    "url": "https://github.com/aleksnyder/wmata-cli/issues"
  },
  "version": "1.0.1",
  "main": "./bin/cli.js",
  "bin": {
    "wmata-cli": "./bin/cli.js",
    "wcli": "./bin/cli.js"
  },
  "files": [
    "app"
  ],
  "scripts": {
    "localdev": "NODE_ENV=development node bin/cli.js",
    "lint": "eslint app",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run lint:fix && npm run testonly:cov",
    "testonly": "NODE_OPTIONS=--experimental-vm-modules NODE_ENV=test jest",
    "testonly:cov": "NODE_OPTIONS=--experimental-vm-modules jest --coverage --runInBand --forceExit --no-cache",
    "testonly:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "chalk": "^2.3.0",
    "cli-table3": "^0.5.1",
    "commander": "^2.19.0",
    "inquirer": "^6.2.1"
  },
  "devDependencies": {
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "^19.0.1",
    "eslint-config-prettier": "^3.4.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.3.0",
    "prettier": "^1.8.2",
    "prettier-package-json": "^1.4.0"
  },
  "keywords": [
    "WMATA",
    "Metro",
    "DC",
    "cli"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "app/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/"
    ],
    "testPathIgnorePatterns": [
      "node_modules/"
    ],
    "transform": {}
  },
  "pkg": {
    "scripts": [
      "app/**/*.js"
    ],
    "targets": [
      "node16-macos",
      "node16-linux",
      "node16-win"
    ]
  }
}
